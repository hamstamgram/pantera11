  !*** ./assets/js/components/background-video.js ***!
  \**************************************************/
/*! exports provided: default */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* WEBPACK VAR INJECTION */(function($) {function setBackgroundImage() {
  var objects = $('.async-image');
  Array.from(objects).map(function (item) {
    // Start loading image
    var img = new Image();
    img.src = item.dataset.src; // if image is loaded replace the src of the HTML element

    img.onload = function () {
      item.classList.remove('async-image');
      item.classList.add('transition-blur');
      return item.style.backgroundImage = "url(".concat(item.dataset.src, ")");
    };
  });
}

var init = function init() {
  var bgVideo = $(".background-video");

  if (bgVideo != null) {
    bgVideo.trigger('play');
    bgVideo.loop = true;
    bgVideo.muted = true;
  }

  setBackgroundImage();
};

