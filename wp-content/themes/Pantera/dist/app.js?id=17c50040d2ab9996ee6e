        sortItem.classList.remove('up-down');
        sortItem.classList.add('down-up');
        return 'up';
      } else {
        sortItem.classList.remove('down-up');
        sortItem.classList.add('up-down');
        return 'down';
      }
    }
  }, {
    key: "sortOption",
    value: function sortOption(filter) {
      switch (filter) {
        // Filter by name: the title of column might change in CMS
        case 'sorting-0':
          this.sortColumnOne();
          break;
        // Filter by type or category: the title of column might change in CMS

        case 'sorting-1':
          this.sortColumnTwo();
          break;
        // Filter by sector or tag: the title of column might change in CMS

        case 'sorting-2':
          this.sortColumnThree();
          break;

        case 'shortName':
          this.sortByShortName();
          break;

        case 'allCategories':
          this.sortByAllCategories();
          break;
      }
    }
