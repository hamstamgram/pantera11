  }, {
    key: "monitorScroll",
    value: function monitorScroll(scrollOffset) {
      //First time going down
      if (scrollOffset > this.navHeight && this.docked) {
        this.docked = false;
        this.unDockMenu();
      } // We are going up, time to stick only if we are no sticky yet and not docked
      else if (this.lastScroll > scrollOffset && !this.docked && !this.sticky) {
        this.sticky = true;
        this.stickMenu();
      } // We are down, time to hide it
      else if (this.lastScroll < scrollOffset && this.sticky) {
        this.sticky = false;
        this.unStickMenu();
      } // We are in the top, no Stick and we Dock
      else if (scrollOffset === 0) {
        this.sticky = false;
        this.docked = true;
        this.unStickMenu();
        this.dockMenu();
      }

      this.lastScroll = scrollOffset;
    }
